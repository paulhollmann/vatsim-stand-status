{
  "name": "paulhollmann/vatsim-stand-status",
  "description": "A PHP library to access the status of aircraft stands on VATSIM",
  "keywords": [
    "vatsim",
    "stand",
    "stands",
    "flight simulator"
  ],
  "homepage": "https://github.com/atoff/vatsim-stand-status",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Alex Toff",
      "email": "alex.toff@cobaltgrid.com",
      "homepage": "https://cobaltgrid.com"
    }
  ],

  "require": {
    "php": "^8.1",
    "ext-ctype": "*",
    "guzzlehttp/guzzle": "^6.5|^7.0",
    "paulhollmann/vatsim-data": "^v1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^1.3",
    "symfony/var-dumper": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "CobaltGrid\\VatsimStandStatus\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
